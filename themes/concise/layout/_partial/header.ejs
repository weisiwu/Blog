<%
    var className = '';
    var hasAside = true;
    if( is_post() ) {
        className = 'post-layout';
        hasAside = false;
    }
%>
<header class="header clearfix <%- className %>">
    <%- partial('nav') %>
    <% if( hasAside ) { %>
        <%- partial('aside') %>
    <% } %>
</header>
